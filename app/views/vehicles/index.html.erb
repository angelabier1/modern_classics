<%- model_class = Vehicle -%>
<div class="page-header">
  <h1><%=t '.title', :default => Vehicle %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:vin) %></th>
      <th><%= model_class.human_attribute_name(:mileage) %></th>
      <th><%= model_class.human_attribute_name(:year) %></th>
      <th><%= model_class.human_attribute_name(:make) %></th>
      <th><%= model_class.human_attribute_name(:model) %></th>
      <th><%= model_class.human_attribute_name(:transmission) %></th>
      <th><%= model_class.human_attribute_name(:cylinders) %></th>
      <th><%= model_class.human_attribute_name(:drivetrain) %></th>
      <th><%= model_class.human_attribute_name(:body_type) %></th>
      <th><%= model_class.human_attribute_name(:fuel_type) %></th>
      <th><%= model_class.human_attribute_name(:body_style) %></th>
      <th><%= model_class.human_attribute_name(:title_in) %></th>
      <th><%= model_class.human_attribute_name(:mileage_is) %></th>
      <th><%= model_class.human_attribute_name(:new_used) %></th>
      <th><%= model_class.human_attribute_name(:exterior_color) %></th>
      <th><%= model_class.human_attribute_name(:interior_color) %></th>
      <th><%= model_class.human_attribute_name(:trim) %></th>
      <th><%= model_class.human_attribute_name(:doors) %></th>
      <th><%= model_class.human_attribute_name(:inventory_source) %></th>
      <th><%= model_class.human_attribute_name(:engine) %></th>
      <th><%= model_class.human_attribute_name(:highway_mpg) %></th>
      <th><%= model_class.human_attribute_name(:wheels) %></th>
      <th><%= model_class.human_attribute_name(:condition) %></th>
      <th><%= model_class.human_attribute_name(:tag_number) %></th>
      <th><%= model_class.human_attribute_name(:photos_taken) %></th>
      <th><%= model_class.human_attribute_name(:active) %></th>
      <th><%= model_class.human_attribute_name(:options) %></th>
      <th><%= model_class.human_attribute_name(:city_mpg) %></th>
      <th><%= model_class.human_attribute_name(:nada_wholesale) %></th>
      <th><%= model_class.human_attribute_name(:nada_retail) %></th>
      <th><%= model_class.human_attribute_name(:sale_status) %></th>
      <th><%= model_class.human_attribute_name(:est_recond_cost) %></th>
      <th><%= model_class.human_attribute_name(:age) %></th>
      <th><%= model_class.human_attribute_name(:comments) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.id %></td>
        <td><%= vehicle.vin %></td>
        <td><%= vehicle.mileage %></td>
        <td><%= vehicle.year %></td>
        <td><%= vehicle.make %></td>
        <td><%= vehicle.model %></td>
        <td><%= vehicle.transmission %></td>
        <td><%= vehicle.cylinders %></td>
        <td><%= vehicle.drivetrain %></td>
        <td><%= vehicle.body_type %></td>
        <td><%= vehicle.fuel_type %></td>
        <td><%= vehicle.body_style %></td>
        <td><%= vehicle.title_in %></td>
        <td><%= vehicle.mileage_is %></td>
        <td><%= vehicle.new_used %></td>
        <td><%= vehicle.exterior_color %></td>
        <td><%= vehicle.interior_color %></td>
        <td><%= vehicle.trim %></td>
        <td><%= vehicle.doors %></td>
        <td><%= vehicle.inventory_source %></td>
        <td><%= vehicle.engine %></td>
        <td><%= vehicle.highway_mpg %></td>
        <td><%= vehicle.wheels %></td>
        <td><%= vehicle.condition %></td>
        <td><%= vehicle.tag_number %></td>
        <td><%= vehicle.photos_taken %></td>
        <td><%= vehicle.active %></td>
        <td><%= vehicle.options %></td>
        <td><%= vehicle.city_mpg %></td>
        <td><%= vehicle.nada_wholesale %></td>
        <td><%= vehicle.nada_retail %></td>
        <td><%= vehicle.sale_status %></td>
        <td><%= vehicle.est_recond_cost %></td>
        <td><%= vehicle.age %></td>
        <td><%= vehicle.comments %></td>
        <td><%=l vehicle.created_at, format: :long %></td>
        <td>
          <%= link_to vehicle_path(vehicle), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to vehicle_path(vehicle), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_vehicle_path,
            :class => 'btn btn-primary' %>
